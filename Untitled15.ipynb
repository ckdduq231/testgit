    "from turtle import *\n",
    "\n",
    "class MyTurtle(Turtle): #MyTurtle 클래스에 Turtle클래스를 상속해서 이용한다.\n",
    "    def drawSquare(self):\n",
    "        for i in range(4):\n",
    "            self.right(90)\n",
    "            self.forward(100)\n",
    "\n",
    "my_turtle = MyTurtle()\n",
    "my_turtle.forward(100)\n",
    "my_turtle.drawSquare()"
   ]
  }
